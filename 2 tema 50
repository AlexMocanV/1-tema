#include <bits/stdc++.h>

using namespace std;

int main()
{
    int n,nr=0,s=0;
    double ma;
    while(cin>>n)
    {
        if(n==0)
        {
            break;
        }
        if(n%2==1)
        {
            nr++;
            s+=n;
        }
    }
    ma=s/nr;
    cout<<ma<<endl;
    cout<<setprecision(2)<<ma;

    return 0;
}
