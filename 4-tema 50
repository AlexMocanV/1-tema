#include <bits/stdc++.h>

using namespace std;

int main()
{
    int n,i,nr=0,p=1,s=0;
   while(cin>>n)
   {
       if(n==0)
       {
           break;
       }
       nr++;
       if(nr%2==0)
       {
           s+=n;
       }
       else
       {
           p*=n;
       }
   }
    cout<<s<<" "<<p; 

    return 0;
}
